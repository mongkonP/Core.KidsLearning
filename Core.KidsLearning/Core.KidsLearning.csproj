<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EPPlus" Version="5.7.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Core.KidsLearning.ChoiceTest\Core.KidsLearning.ChoiceTest.csproj" />
    <ProjectReference Include="..\Core.KidsLearning.Classed\Core.KidsLearning.Classed.csproj" />
    <ProjectReference Include="..\Core.KidsLearning.Control\Core.KidsLearning.Control.csproj" />
    <ProjectReference Include="..\Core.KidsLearning.Print\Core.KidsLearning.Print.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="TORServices.DB">
      <HintPath>bin\Debug\net5.0-windows\TORServices.DB.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Drawings">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Drawings.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Ext">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Ext.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Forms">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Forms.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Maths">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Maths.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Network">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Network.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.PathFile">
      <HintPath>bin\Debug\net5.0-windows\TORServices.PathFile.dll</HintPath>
    </Reference>
    <Reference Include="TORServices.Systems">
      <HintPath>bin\Debug\net5.0-windows\TORServices.Systems.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="frm\frmMain.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="frm\frmMain.Designer.cs">
      <DependentUpon>frmMain.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="frm\frmMain.resx">
      <DependentUpon>frmMain.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="clss\plnt\prnMath\" />
  </ItemGroup>

</Project>